<p id="notice"><%= notice %></p>

<h1>Inks</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th>Manufacturer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @inks.each do |ink| %>
      <tr>
        <td><%= ink.name %></td>
        <td><%= ink.model %></td>
        <td><%= ink.manufacturer %></td>
        <td><%= link_to 'Show', ink %></td>
        <td><%= link_to 'Edit', edit_ink_path(ink) %></td>
        <td><%= link_to 'Destroy', ink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ink', new_ink_path %>
